{
  "name": "users",
  "table": "ca_users",
  "attr": {
    "id": {
      "type": "int",
      "auto_increment": true,
      "primary_key": true,
      "allow_null": false
    },
    "username": {
      "type": "varchar",
      "size": 255,
      "allow_null": false,
      "unique": true
    },
    "password": {
      "type": "varchar",
      "size": 255,
      "allow_null": false
    },
    "email": {
      "type": "varchar",
      "size": 255,
      "allow_null": false,
      "unique": true
    },
    "first_name": {
      "type": "varchar",
      "size": 255,
      "allow_null": false
    },
    "last_name": {
      "type": "varchar",
      "size": 255,
      "allow_null": false
    },
    "created_at": {
      "type": "datetime",
      "allow_null": false
    },
    "updated_at": {
      "type": "datetime",
      "allow_null": false
    }
  },
  "indexes": ["username", "email"],
  "operations": {
    "select": {
      "query": "SELECT * FROM ca_users WHERE id = ?",
      "params": ["id"]
    }
  }
}
